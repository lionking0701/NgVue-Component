{
  "name": "ngVue",
  "author": "Doray Hong <hongduhui@gmail.com>",
  "version": "0.1.2",
  "description": "Use Vue Components in Angular 1.x",
  "main": "build/ngVue.umd.js",
  "jsnext:main": "build/ngVue.es.js",
  "keywords": [
    "javascript",
    "migration",
    "vue",
    "angular",
    "ng",
    "components",
    "ngvue"
  ],
  "scripts": {
    "test": "BABEL_ENV=test mocha --compilers js:babel-core/register --recursive",
    "dev-example": "npm run bundle && BABEL_ENV=dev webpack-dev-server --inline --hot --config ./example/webpack.config.js --content-base ./example",
    "clean": "rm -rf build",
    "minify": "uglifyjs ./build/ngVue.umd.js -c -m -o ./build/ngVue.min.js",
    "bundle": "BABEL_ENV=build rollup -c",
    "lint": "eslint src lib test example",
    "build": "npm run lint && npm run clean && npm run bundle && npm run minify",
    "preversion": "npm run test",
    "prepublish": "npm run build"
  },
  "dependencies": {
    "angular": "^1.5.8",
    "vue": "^2.0.0-beta.5"
  },
  "devDependencies": {
    "babel-core": "^6.1.21",
    "babel-eslint": "^4.1.5",
    "babel-helper-vue-jsx-merge-props": "^2.0.1",
    "babel-loader": "^6.0.0",
    "babel-plugin-syntax-jsx": "^6.13.0",
    "babel-plugin-transform-vue-jsx": "^3.1.0",
    "babel-preset-es2015": "^6.1.18",
    "babel-preset-es2015-rollup": "^1.1.1",
    "babel-preset-react": "^6.11.1",
    "babel-preset-stage-1": "^6.13.0",
    "chai": "^3.4.1",
    "cross-env": "^1.0.6",
    "css-loader": "^0.23.1",
    "eslint": "^2.0.0",
    "eslint-config-standard": "^5.3.5",
    "eslint-plugin-promise": "^2.0.0",
    "eslint-plugin-react": "^6.0.0",
    "eslint-plugin-standard": "^2.0.0",
    "file-loader": "^0.8.4",
    "mocha": "^2.3.4",
    "rollup": "^0.34.1",
    "rollup-plugin-babel": "^2.6.1",
    "sinon": "^1.17.5",
    "sinon-chai": "^2.8.0",
    "uglify-js": "^2.7.0",
    "vue-loader": "^9.2.2",
    "webpack": "^2.1.0-beta.20",
    "webpack-dev-server": "^2.1.0-beta.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ngVue/ngVue.git"
  },
  "bugs": {
    "url": "https://github.com/ngVue/ngVue/issues"
  },
  "homepage": "https://github.com/ngVue/ngVue#readme",
  "license": "MIT"
}
